#include <iostream>
using namespace std;

 
int
main (){
  
float belahketupat, lingkaran, diagonal, jarijari, luasarsir, phi;
  
 
phi = 3.14;
  
diagonal = 20;
  
jarijari = 0.5 * 20;
  
 
lingkaran = phi * jarijari * jarijari;
  
belahketupat = 0.5 * diagonal * diagonal;
  
 
luasarsir = lingkaran - belahketupat;
  
 
cout << "Luas Daerah Yang Diarsir Adalah : " << luasarsir;
  
 
return 0;

 
 
}
